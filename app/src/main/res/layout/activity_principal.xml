<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.gfili.limpae.MainActivity"
    android:orientation="vertical">

           <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:orientation="vertical">

    <TextView
        android:id="@+id/tvBemVindo"
        android:layout_width="329sp"
        android:layout_height="36sp"
        android:text="Login efetuado com sucesso"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="16sp"
        tools:layout_editor_absoluteY="16sp" />

    <TextView
        android:id="@+id/tvUsuario"
        android:layout_width="92sp"
        android:layout_height="31sp"
        android:text="Username"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="16sp"
        tools:layout_editor_absoluteY="67sp" />

    <EditText
        android:id="@+id/etUsername"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="16sp"
        tools:layout_editor_absoluteY="98sp"/>

    </LinearLayout>
</LinearLayout>
